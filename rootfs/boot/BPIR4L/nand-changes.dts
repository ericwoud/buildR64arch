/dts-v1/;
/plugin/;

#define MTK_PUPD_SET_R1R0_11 103

&spi2_flash_pins {
    conf-pu {
			pins = "SPI2_CS", "SPI2_HOLD", "SPI2_WP";
			drive-strength = <4>;
			bias-pull-up = <MTK_PUPD_SET_R1R0_11>;
		};

		conf-pd {
			pins = "SPI2_CLK", "SPI2_MOSI", "SPI2_MISO";
			drive-strength = <4>;
			bias-pull-down = <MTK_PUPD_SET_R1R0_11>;
		};
};

&spi_nand {
    spi-cs-setup-delay-ns = <15>;
    spi-cs-hold-delay-ns = <15>;
    /* remove from main .dtb as overlay file cannot delete a property */
    //fdt___put -d /soc/spi@11009800/flash@0/partitions/partition@0 read-only
    // disabled for now, it is not set to read-only
};
